{
  "name": "dooper-bootcamp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "generate": "yarn plop",
    "types": "npx swagger-typescript-api -p $(grep NEXT_PUBLIC_SUPABASE_URL .env.local | cut -d '=' -f2)/rest/v1/?apikey=$(grep NEXT_PUBLIC_SUPABASE_ANON_KEY .env.local | cut -d '=' -f2) -o ./src/types -n SchemaDB.ts --no-client"
  },
  "dependencies": {
    "@emotion/cache": "latest",
    "@emotion/react": "latest",
    "@emotion/server": "latest",
    "@emotion/styled": "latest",
    "@fontsource/roboto": "^4.5.5",
    "@mui/icons-material": "latest",
    "@mui/material": "latest",
    "@supabase/supabase-auth-helpers": "^1.3.0",
    "@supabase/supabase-js": "^1.33.3",
    "formik": "^2.2.9",
    "next": "latest",
    "react": "latest",
    "react-dom": "latest",
    "react-query": "^3.38.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/node": "latest",
    "@types/react": "latest",
    "eslint": "latest",
    "eslint-config-next": "latest",
    "eslint-config-prettier": "^8.5.0",
    "husky": ">=6",
    "inquirer-directory": "^2.2.0",
    "lint-staged": ">=10",
    "plop": "^3.0.5",
    "prettier": "2.6.2",
    "typescript": "latest"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
